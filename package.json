{
  "name": "aalib.js",
  "version": "2.0.0",
  "description": "ASCII art library in JavaScript",
  "main": "dist/aalib.js",
  "module": "lib/index.js",
  "author": "mirz <that.mirz@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/mir3z/aalib.js",
  "scripts": {
    "dev": "npx webpack serve --static . -c ./dev/webpack.config.js --open-target ./dev",
    "dev:playground": "npx webpack serve --static . -c playground/webpack.config.js",
    "build": "npm run build:dist && npm run build:lib",
    "build:dist": "webpack --progress",
    "build:lib": "babel src --out-dir lib",
    "clean": "rm -rf lib dist",
    "test": "karma start",
    "test:perf": "node ./test/perf/bin/run-perf.js",
    "lint": "eslint src/** test/**",
    "build:dev": "webpack --config dev/webpack.config.js",
    "push-dev-to-lw": "npm run build:dev && ./push-dev-to-lw.sh"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.3",
    "babel-preset-env": "^1.7.0",
    "benchmark": "^2.1.4",
    "body-parser": "^1.20.2",
    "chai": "^5.1.1",
    "eslint": "^9.5.0",
    "karma": "^6.4.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.1",
    "lodash": "^4.17.21",
    "mocha": "^10.4.0",
    "sinon": "^18.0.0",
    "vue-loader": "^17.4.2",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "path-browserify": "^1.0.1",
    "querystring-es3": "^0.2.1",
    "rxjs": "^5.4.3",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "vue": "^2.5.7"
  }
}
